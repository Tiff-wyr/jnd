/**app.wxss**/
@import "/components/tabBar/index.wxss";
@import "/static/iconfont/icon/iconfont.wxss";

page{
  width: 100%;
  height: 100%;
    background:#FAFAFB;
}
.bottom{
  position: fixed;
  left:0;
  right:0;
  bottom: 0;
width:100%;
height:140rpx;
background:rgba(255,255,255,1);
z-index: 998;
padding: 22rpx 36rpx;
box-sizing: border-box;
}
.bottom button{
height:96rpx;
background:rgba(205,69,69,1);
border-radius:12rpx;
line-height:96rpx;
font-size:28rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(255,255,255,1);
}
.picker {
  display: flex;
  font-size: 24rpx;
  width: 100rpx;
  height: 70rpx;
  line-height: 70rpx;
}
.triangle {
  border: 10rpx solid transparent;
  border-top-color: #999;
  margin: 30rpx 5rpx
}
.head-wrap {
  display: flex;
  width: 100%;
  height: 70rpx;
  flex-direction: row
}
.search-wrap {
  flex: 1;
  display: flex;
  height: 70rpx;
  background: #eee;
  border-radius: 20rpx;
  margin-left: 30rpx;
  box-sizing: border-box;
  padding-top:10rpx 

}
.search-img {
  width: 70rpx;
  height: 70rpx;
  font-size:32rpx;
  text-align:center;
  line-height:50rpx;
  color:#666;
}
.search-input {
font-size:26rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(158,158,158,1);

}
.red {
  color: #CD4545;
}
button{
  padding: 0 3rpx 0 3rpx;
}