<!--pages/agent/personMessage/personMessage.wxml-->

<view class='person'>
<view class='per-it'>
  <view class='person-item'>
    <view class='name'>名称</view>
    <view class='data'>{{personData.agencyName}}</view>
  </view>
  <view class='person-item'>
    <view class='name'>机构代码</view>
    <view class='data'>{{personData.agencyCode}}</view>
  </view>

    <view class='introduction-item'>
    <view class='name'>机构简介</view>
    <view class='introduction'>{{personData.agencyIntroduction}}</view>
  </view>



  <view class='good-item'>
    <view class='name'>营业期限</view>
    <view class='business'>{{personData.startBusiness}}~{{personData.endBusiness}}</view>
  </view>

    <view class='person-item'>
    <view class='name'>机构电话</view>
    <view class='data'>{{personData.agencyPhone}}</view>
  </view>

  <view class='person-item'>
    <view class='name'>机构网址</view>
    <view class='data'>{{personData.agencyURL}}</view>
  </view>
  <view class='person-item'>
    <view class='name'>所属公司</view>
    <view class='data'>{{personData.companyOwned}}</view>
  </view>



  <view class='person-item'>
    <view class='name'>机构所在地</view>
    <view class='data'>{{personData.address}}</view>
  </view>
  <view class='person-item'>
    <view class='name'>手机号</view>
    <view class='data'>{{personData.phone}}</view>
  </view>
  <view class='person-item'>
    <view class='name'>邮箱</view>
    <view class='data'>{{personData.email}}</view>
  </view>
</view>


 <view class='bottom'>
      <button bindtap='look'>登陆PC端修改个人信息</button>
 </view>  

  <view class='mask' wx:if='{{isMask}}'>
      </view>
      <view class='com-bro' wx:if='{{isMask}}'>
          <view class='text1'>在电脑浏览器中打开</view>
          <view class='text2'>www.9nengdai.com</view>
          <button bindtap='cancel'>确  定</button>
      </view>

</view>