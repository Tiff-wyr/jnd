<view class='pass'>
  <image src='/static/icon/tu.png'></image>
  <view class='wrap'>
   <view class='iconfont icon-shoujihao-copy duanPic'></view>
    <input class='input' placeholder-class='input_placeholder'  bindblur='handlePhone' placeholder='输入手机号'></input>
  </view>
  <view class='wrap'>
     <view class='iconfont icon-biyan1-copy-copy-copy duanPic'></view>
    <input class='input' placeholder-class='input_placeholder' bindblur='handlePassword' password='true' placeholder='输入密码'></input>
  </view>

  <button bindtap='login'>登   陆</button>
  <view class='bottom-pass'>
       <view class='register' bindtap='freeRe'>免费注册</view>
          <view class='register' bindtap='msgLogin'>短信登陆</view>
  </view>
</view>