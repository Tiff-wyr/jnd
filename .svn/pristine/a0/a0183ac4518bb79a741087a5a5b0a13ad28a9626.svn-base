<!--pages/agent/personMessage/personMessage.wxml-->

<view class='person'>
<view class='per-mar'>
    <view class='person-item'>
    <view class='name'>姓名</view>
    <view class='data'>{{personData.brokerName}}</view>
  </view>
  <view class='person-item'>
    <view class='name'>性别</view>
    <view class='data'>{{personData.sexValue}}</view>
  </view>
  <view class='person-item'>
    <view class='name'>年龄</view>
    <view class='data'>{{personData.age}}</view>
  </view>
  <view class='person-item'>
    <view class='name'>身份证号</view>
    <view class='data'>{{personData.identity}}</view>
  </view>
  <view class='good-item'>
    <view class='name'>擅长业务</view>
    <view class='business'>{{personData.businessScopeInfo}}</view>
  </view>

  <view class='person-item'>
    <view class='name'>所属公司</view>
    <view class='data'>{{personData.company}}</view>
  </view>
  <view class='person-item'>
    <view class='name'>从业年限</view>
    <view class='data'>{{personData.workingYears}}年</view>
  </view>

  <view class='introduction-item'>
    <view class='name'>个人介绍</view>
    <view class='introduction'>{{personData.introduction}}</view>
  </view>

  <view class='person-item'>
    <view class='name'>所在地区</view>
    <view class='data'>{{personData.address}}</view>
  </view>
  <view class='person-item'>
    <view class='name'>手机号</view>
    <view class='data'>{{personData.phone}}</view>
  </view>
  <view class='person-item'>
    <view class='name'>邮箱</view>
    <view class='data'>{{personData.email}}</view>
  </view>

</view>


 <view class='bottom'>
      <button bindtap='look'>登陆PC端修改个人信息</button>
 </view>  
      <view class='mask' wx:if='{{isMask}}'>
      </view>
      <view class='com-bro' wx:if='{{isMask}}'>
          <view class='text1'>在电脑浏览器中打开</view>
          <view class='text2'>www.9nengdai.com</view>
          <button bindtap='cancel'>确  定</button>
      </view>

</view>