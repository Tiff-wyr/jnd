/**app.wxss**/
@import "/components/tabBar/index.wxss";
@import "/static/iconfont/icon/iconfont.wxss";

page{
  width: 100%;
  height: 100%;
    background:#FAFAFB;
}
.bottom{
  position: fixed;
  left:0;
  right:0;
  bottom: 0;
width:100%;
height:140rpx;
background:rgba(255,255,255,1);
z-index: 998;
padding: 22rpx 36rpx;
box-sizing: border-box;
}
.bottom button{
height:96rpx;
background:rgba(205,69,69,1);
border-radius:12rpx;
line-height:96rpx;
font-size:28rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(255,255,255,1);
}

.picker {
  display: flex;
  font-size: 24rpx;
  height: 100rpx;
  line-height: 100rpx;
}
.triangle {
  border: 10rpx solid transparent;
  border-top-color: #999;
  margin: 45rpx 5rpx
}
.mb100{
margin-bottom: 100rpx
}
.head-wrap {
  display: flex;
  width: 100%;
  height: 70rpx;
  flex-direction: row
}
.search-wrap {
  flex: 1;
  display: flex;
  height: 70rpx;
  background: #eee;
  border-radius: 20rpx;
  margin-left: 30rpx;
  box-sizing: border-box;
  padding-top:10rpx ;
  padding-left: 20rpx;

}
.search-img {
  width: 70rpx;
  height: 70rpx;
  font-size:32rpx;
  text-align:center;
  line-height:50rpx;
  color:#666;
  margin-left: 20rpx;
}
.search-input {
font-size:26rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(158,158,158,1);

}
.size{
  width: 40rpx;
  height: 40rpx;
   line-height: 124rpx
}

button{
  padding: 0 3rpx 0 3rpx;
}
.red {
  color: #CD4545;
}
.mask{
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  background: #000;
  z-index: 100;
  opacity: 0.1
}
.com-bro{

height:368rpx;
background:rgba(255,255,255,1);
    position: fixed;
      left: 0;
  bottom: 0;
  right: 0;
  z-index: 998;
  text-align: center;
  padding: 46rpx 36rpx 22rpx 36rpx;
  box-sizing: border-box;

}
.com-bro .text1{
height:32rpx;
font-size:32rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(0,0,0,1);
line-height:32rpx;

}

.com-bro .text2{
height:32rpx;
font-size:32rpx;
font-family:PingFangSC-Medium;
font-weight:500;
color:rgba(205,69,69,1);
line-height:32rpx;
margin: 40rpx 0 100rpx 0;
}
.com-bro button{
   width:678rpx;
height:96rpx;
background:rgba(244,67,54,1);
border-radius:12rpx;
margin: 0 auto;
font-size:28rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(255,255,255,1);
line-height:96rpx;
}
.wrapper .code{
  flex: 1;
  margin-right: 20rpx;
  padding: 20rpx 30rpx;
box-sizing: border-box;
height:96rpx;
background:rgba(255,255,255,1);
border-radius:4rpx;
border:1rpx solid rgba(199,199,199,1);
display: flex;
}
.code .input{
  flex: 1;
  margin-left:15rpx;
    font-size: 28rpx;
 color:#333;
}
.wrapper .sendCode{
  text-align: center;
  width:244rpx;
height:96rpx;
background:rgba(255,255,255,1);
border-radius:4rpx;
border:1rpx solid rgba(199,199,199,1);
font-size:28rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(205,69,69,1);
line-height:96rpx;
}

.input_placeholder {
font-size:28rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(164,164,164,1);
line-height:28rpx;
}
.wrapper .code .img2{
     width: 38rpx;
height: 19rpx;
  line-height: 56rpx;
  margin-top:19rpx;
}
.wrapper{
  display: flex;
  width:100%;
margin: 40rpx 0 30rpx 0;
}
.city{
  display: flex;
  justify-content: space-between;
  margin-top: 30rpx;

}
.city .city-left{
    display: flex;
padding:20rpx 30rpx;
box-sizing:border-box;
  width:325rpx;
height:96rpx;
background:rgba(255,255,255,1);
border-radius:4rpx;
border:1rpx solid rgba(199,199,199,1);
}
.city .city-left .img1{
        width: 28rpx;
height: 40rpx;
  line-height: 56rpx;
  margin-top:7rpx;
}
.city .city-right{
  margin-left: 35rpx;
  padding:20rpx 30rpx;
box-sizing:border-box;
   flex: 1;

   height:96rpx;
background:rgba(255,255,255,1);
border-radius:4rpx;
border:1rpx solid rgba(199,199,199,1);
}
.radio-group {
  margin-top: 20rpx; 
  display: flex;
  justify-content: space-between
}
.checkbox-group {
    margin-top: 20rpx; 
}
.pawns{
  display: flex;
  justify-content: space-between;
  margin-top: 30rpx;
}

.pawns .pawn-middle,.pawn-left,.pawn-right{
padding:20rpx 10rpx;
box-sizing:border-box;
height:76rpx;
min-width: 200rpx;
background:rgba(255,255,255,1);
border-radius:4rpx;
border:1rpx solid rgba(199,199,199,1);
}
.pawn-middle{
  margin-left: 10rpx;
}
.pawn-right{
  margin-left: 10rpx;
}

.kong{
  text-align: center
}
.kong image{
  width: 310rpx;
  height:240rpx;
  margin: 80rpx auto;
}
.kong .accord{
height:32rpx;
font-size:32rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(74,74,74,1);
line-height:32rpx;
margin-top: 60rpx
}
