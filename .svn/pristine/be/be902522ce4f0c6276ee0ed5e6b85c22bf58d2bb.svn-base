.pass{
  padding: 50rpx 36rpx;
  box-sizing: border-box;
}
.pass image{
  width:100%;
height:340rpx;
}
.city{
  display: flex;
  justify-content: space-between;
  margin-top: 30rpx;

}
.city .city-left{
    display: flex;
padding:20rpx 30rpx;
box-sizing:border-box;
  width:354rpx;
height:96rpx;
background:rgba(255,255,255,1);
border-radius:4rpx;
border:1rpx solid rgba(199,199,199,1);
}
.city .city-left .img1{
        width: 28rpx;
height: 40rpx;
  line-height: 56rpx;
  margin-top:7rpx;
}
.city .city-right{
  margin-left: 20rpx;
  padding:20rpx 30rpx;
box-sizing:border-box;
   flex: 1;
   height:96rpx;
background:rgba(255,255,255,1);
border-radius:4rpx;
border:1rpx solid rgba(199,199,199,1);
}

.top-re{
  display: flex;
  justify-content: space-between;
    margin-top: 30rpx;
}
.top-re .name-left{
    display: flex;
padding:20rpx 30rpx;
box-sizing:border-box;

  width:354rpx;
height:96rpx;
background:rgba(255,255,255,1);
border-radius:4rpx;
border:1rpx solid rgba(199,199,199,1);
}
.top-re .name-left .img1{
      width: 28rpx;
height: 40rpx;
  line-height: 56rpx;
  margin-top:7rpx;
}
.top-re .name-left .name-input{
    margin-left: 15rpx;
  flex: 1;
  font-size: 28rpx;
   color:#333;
}

.wrapper{
  display: flex;
  width:100%;
margin: 40rpx 0 30rpx 0;
}
.wrapper img1{
    width: 28rpx;
height: 40rpx;
  line-height: 56rpx;
  margin-top:7rpx;
}
.wrapper .code .img2{
     width: 38rpx;
height: 19rpx;
  line-height: 56rpx;
  margin-top:19rpx;
}
.pass .wrap{
  width:100%;
  height:96rpx;
  background:rgba(255,255,255,1);
  border-radius:4rpx;
  border:1rpx solid rgba(199,199,199,1);
  display: flex;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  margin: 40rpx 0 30rpx 0;
}
.wrap .img1{
  width: 28rpx;
height: 40rpx;
  line-height: 56rpx;
  margin-top:7rpx;
}
.wrap .input{
  margin-left: 15rpx;
  flex: 1;
  font-size: 28rpx;
   color:#333;
}
.wrapper .code{
  flex: 1;
  margin-right: 20rpx;
  padding: 20rpx 30rpx;
box-sizing: border-box;
height:96rpx;
background:rgba(255,255,255,1);
border-radius:4rpx;
border:1rpx solid rgba(199,199,199,1);
display: flex;
}
.code .input{
  flex: 1;
  margin-left:15rpx;
    font-size: 28rpx;
 color:#333;
}


.wrapper .sendCode{
  text-align: center;
  width:244rpx;
height:96rpx;
background:rgba(255,255,255,1);
border-radius:4rpx;
border:1rpx solid rgba(199,199,199,1);
font-size:28rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(205,69,69,1);
line-height:96rpx;
}
.input_placeholder {
font-size:28rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(164,164,164,1);
line-height:28rpx;
}
.pass button{
  width:100%;
height:96rpx;
background:rgba(244,67,54,1);
border-radius:12rpx;
font-size:32rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(255,255,255,1);
line-height:96rpx;
margin: 80rpx 0 20rpx 0;
}

 .register{
   width: 100%;
font-size:28rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:#A4A4A4;
line-height:28rpx;
text-align: center
}

 .register text{
   color: #CD4545
 }
