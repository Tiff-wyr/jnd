/* components/tabBar/index.wxss */
.icon{
  width:54rpx;
  height: 54rpx;
}
.tabBar{
  width:100%;
  height: 99rpx;
  position: fixed;
  bottom:0;
  left: 0;
  padding:10rpx;
  margin-left:-4rpx;
  background: #fff;
  font-size:20rpx;
  color:#8A8A8A;
  box-shadow: 6rpx 6rpx 6rpx 6rpx #aaa;
}

 .tabBar-item{
  float:left;
  width:25%;
  text-align: center;
  overflow: hidden;
}
.text{
  margin-top: 14rpx;
    color:#8A8A8A;
}
/*当前字体颜色*/
.tabBartext{
  color:red;
}