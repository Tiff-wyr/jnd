/* borrower/pages/applyAccord/applyAccord.wxss */
page{
 width: 100%;
height:100% ;
background: #FAFAFB;

}
.accord{
padding: 30rpx 36rpx;
box-sizing: border-box;

}
.accord .accord-item{
  padding: 30rpx 30rpx;
  box-sizing: border-box;
height:180rpx;
background:rgba(255,255,255,1);
box-shadow:0px 3rpx 8rpx 0px rgba(215,215,215,1);
border-radius:10rpx;
margin-bottom: 30rpx;
}
.wrap{
  display: flex;
  justify-content: space-between
}
.wrap image{
  width: 120rpx;
  height: 120rpx;
  margin-right: 20rpx;
}
.wrap .name{
font-size:32rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(69,69,69,1);
line-height:32rpx;
margin-top: 16rpx;
}
.wrap .money{
font-size:24rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(123,123,123,1);
line-height:32rpx;
margin-top: 24rpx;
}
.wrap .phone{
font-size:32rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(69,69,69,1);
line-height:120rpx;

}
