<!--pages/agent/borrower/borrower.wxml-->
<import src="/components/tabBar/index.wxml"/>
<view clsss="borrower">

  <view class='top'>
    <view class='borr-top'>
      <view>北京</view>
      <view class='arrow'>55</view>
    </view>

    <view class='borr-middle'>
      <view>最新</view>
      <view>金额</view>
    </view>
  </view>



  <view wx:for="{{tableData}}" wx:key="{{index}}" bindtap='handlerClick' class='borr-bottom'>
    <view class='wrap'>
      <view>
        <view class='name'>{{item.name}}</view>
        <view class='address'>{{item.address}}</view>
        <view class='address'>贷款金额：{{item.amount}}</view>
      </view>
      <button class='btn'>立即沟通</button>

    </view>
    <view class='add'></view>
  </view>
  
  <template is="tabBar" data="{{tabBar:bindData.tabBar}}"/>
</view>