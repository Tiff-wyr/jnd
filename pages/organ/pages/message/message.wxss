.list_left{
  height: 144prx;
  display: flex
}
.list_left .line{
    padding-top: 10rpx;
    display: flex;
    justify-content:space-around;
    font-size: 24rpx;
    flex: 1
}
.noData{
    text-align: center
}
.noDataText{
width: 100%;
height:32rpx;
font-size:32rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(74,74,74,1);
line-height:32rpx;
margin-top: 100rpx
}
.content{
  width: 200rpx;
  height: 30rpx;
    line-height: 30rpx;
  overflow: hidden
}
.chat_list {
	width: 100%;
	height: 144rpx;
  padding-left: 30rpx;
  box-sizing: border-box;
  background-color: #fff; 
}
.list_user{
    font-size: 30rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    display: inline-block;
    margin-bottom: 6rpx;
    color: #333333;
    line-height: 42rpx;
}

.list_text {
	flex: 1;
	font-size: 4rpx;
    border-bottom: 1px solid #ececec;
    height: 130rpx;
}
.list_pic {
	position: relative;
	float: left;
	margin-top: 28rpx;
	margin-right: 28rpx;
}
.list_pic image {
	display: block;
	width: 88rpx;
	height: 88rpx;
  border-radius: 50%;
	box-sizing: border-box;
	/* border: 1px solid rgb(0,186,110); */
}
.em-msgNum {
	width: 20px;
	height: 20px;
	border-radius: 10px;
	background: #f04134;
	color: #fff;
	line-height: 20px;
	font-size: 14px;
	text-align: center;
  margin-left:80rpx;
   margin-top:20rpx;
}

